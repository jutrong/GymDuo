{
  "expo": {
    "name": "GymDuo",
    "slug": "GymDuo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": {
      "noto-sanas": "./assets/fonts/NotoSansKR-VariableFont_wght.ttf",
      "roboto-Medium": "./assets/fonts/Roboto-Medium.ttf",
      "roboto-Bold": "./assets/fonts/Roboto-Bold.ttf",
      "roboto-Light": "./assets/fonts/Roboto-Light.ttf",
      "roboto-Black": "./assets/fonts/Roboto-Black.ttf",
      "roboto-Mono": "./assets/fonts/RobotoMono-VariableFont_wght.ttf"
    }
  }
}
